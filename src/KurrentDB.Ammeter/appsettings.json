{
	"ParallelLimit": -1, // -1 limits to Environment.ProcessorCount
	"Node": {
		"Insecure": false,
		"Username": "admin",
		"Password": "changeit", // or via env NODE__PASSWORD
		"NodeType": "embedded", // container, embedded, external
		"Container": {
			"CleanUp": true,
			"Registry": "docker.kurrent.io/kurrent-latest",
			"Repository": "kurrentdb",
			"Tag": "25.1.0"
		},
		"Embedded": {
			"TrustedRootCertificatesPath": "D:/Kurrent/cluster/certs/ca",
			"CertificateFile": "D:/Kurrent/cluster/certs/node3/node.crt",
			"CertificatePrivateKeyFile": "D:/Kurrent/cluster/certs/node3/node.key"
		},
		"External": {
			"Host": "localhost",
			"Port": 2113
		}
	},
	"ExpectedVersion": "" // set this to specify an expected version prefix
}
